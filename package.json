{
  "name": "@saulx/diff",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "ava --verbose"
  },
  "repository": "https://github.com/atelier-saulx/selva",
  "lint-staged": {
    "{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.ts",
      "!test/examples"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@saulx/hash": "^1.0.1",
    "@saulx/utils": "^2.0.1"
  },
  "prettier": "@saulx/prettier-config",
  "eslintConfig": {
    "extends": "@saulx/eslint-config"
  },
  "devDependencies": {
    "@saulx/eslint-config": "https://github.com/atelier-saulx/eslint-config",
    "@saulx/prettier-config": "https://github.com/atelier-saulx/prettier-config",
    "ava": "3.15.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.4"
  }
}
